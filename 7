#include<iostream>
using namesapce std;
int main(){
{
int n,i;
cout<<"enter the number :";
cin>>n;
bool Primeistrue=true;
if(n<=1){
    Primeistrue=false;
}
else{
    for(i=2;i<=n/2;i++){
        if(n%i==0){
            Primeistrue=false;
            break;
        }
    }
}
if(Primeistrue){
    cout<<"Prime number"<<n<<endl;
}
else{
    cout<<" not a prime number";
} return 0;
}          
